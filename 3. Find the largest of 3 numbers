#include<stdio.h>

int main(){
  int i,j,k;
  printf("Enter 1st number: ");
  scanf("%d",&i);
  printf("Enter 2nd number: ");
  scanf("%d",&j);
  printf("Enter 3rd number: ");
  scanf("%d",&k);
  
  if(i>j && i>k){
    printf("largest element is %d ",i);
  }else if(i<j && k<j){
    printf("largest element is %d ",j);
  }else if(k>i && k>j){
    printf("largest element is %d ",k);
  }else if(i=j=k){
    printf("The given number is same");
  }else{
      printf("Error! Enter number not character");
  }
  return 0;
}
